    <viewUtil:IsPrimaryToVisibilityConverter x:Key="IsPrimaryToVisibilityConverter" />
    <viewUtil:IsForeignToVisibilityConverter x:Key="IsForeignToVisibilityConverter" />

    <util1:ColumnsFilterConverter x:Key="ColumnsFilterConverter" />


    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />

    <!--    <dtu:DataGridCellExtendedPropertiesReaderConverter x:Key="DataGridCellExtendedPropertiesReaderConverter"/>-->
    <util2:DataGridCellExtendedPropertiesConverter x:Key="DataGridCellExtendedPropertiesConverter" />
    <util2:DataGridCellExtendedPropertiesConverterHelper x:Key="IsForeingKey"

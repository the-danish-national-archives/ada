    <viewModel:CheckTreeItem x:Key="Here" ShortDesc="Alle test" PreExpanded="true">
<viewModel:CheckTreeItem ShortDesc="0 Ukendte fejl" PreExpanded="false">
    <viewModel:CheckTreeItem ShortDesc="0.1" InternalTestName="Ukendte fejl"
                             Check="AdaAvInternalError"
                             Desc="Fejl som disse optræder i de tilfælde, når ADA uventet ikke kan teste en del af eller hele arkiveringsversionen. Det kan være, at det er på grund af en fejl i arkiveringsversionen, en fejl i ADA-koden eller eksempelvis hvis tilkobling til testmedie pludselig forsvinder. Fejlmeddelelsen vil sjældent være umiddelbart forståelig og er ikke en del af det kendte udfaldsrum af fejlmeddelelser som ADA giver brugeren om fejl i arkiveringsversionen.  Fejl som disse eller andre uhensigtsmæssigheder bør meldes til ada-meldinger@sa.dk."
                             FormattedText="Ukendt fejl i {Module}¤{Exception}" />
    <viewModel:CheckTreeItem ShortDesc="0.2" InternalTestName="Ukendte XML fejl"
                             Check="AdaAvXmlViolation"
                             Desc="Der registreres fejl ved xml indlæsning."
                             FormattedText="Ukendt fejl ved læsning af * {FileName}¤{Exception}" />
    </viewModel:CheckTreeItem>
                <viewModel:CheckTreeItem ShortDesc="4 Datastrukturer" PreExpanded="true">
            <viewModel:CheckTreeItem ShortDesc="4.A.1 Generelle regler om datastrukturer">
                <viewModel:CheckTreeItem.Desc>
                    <system:String xml:space="preserve">I en arkiveringsversion skal alle nøgler have en entydig identifikator. Der må ikke forekomme situationer, hvor det er nødvendigt at uddrage dele af nøglefelter for at forstå it-systemets indhold eller funktion.</system:String>
                </viewModel:CheckTreeItem.Desc>
                <viewModel:CheckTreeItem ShortDesc="4.A_1_1" InternalTestName="Primærnøgletest unik"
                                         Check="PrimaryKeyDuplikates"
                                         Desc="Der testes for, om der findes primærnøgler, der ikke er unikke. Testen foretages ved sammensatte nøgler som enkelt felter, og ikke som en konkatenering af tekststrenge"
                                         FormattedText="Primærnøglefejl, nøgle er ikke unik  {ConstraintName}, {TableName}, {ColumnName} antallet af nøglefejl udgør {Count} %" />
                <viewModel:CheckTreeItem ShortDesc="4.A_1_2" InternalTestName="Primærnøgletest NULL"
                                         Check="PrimaryKeyNull"
                                         Desc="Der testes for, om der findes felter i primærnøglen, der har en NULL værdi"
                                         FormattedText="Primærnøglefejl, nøgle indeholder felter med NULL-værdi {ConstraintName}, {TableName}, {ColumnName} antallet af nøglefejl udgør {Count} %" />
                <viewModel:CheckTreeItem ShortDesc="4.A_1_3" InternalTestName="Primærnøgletest, blank"
                                         Check="PrimaryKeyBlanks"
                                         Desc="Der testes for, om primærnøglefelter starter eller afslutter, med en eller anden form for blanktegn"
                                         FormattedText="Primærnøglefejl. Nøgle begynder eller afslutter med et blanktegn {ConstraintName}, {TableName}, {ColumnName} antallet af nøglefejl udgør {Count} %" />
                <!--        Deprecated        <viewModel:CheckTreeItem ShortDesc="4.A_1_4" Check="PrimaryKeyErrorSummary"  FormattedText=""/>-->
                <viewModel:CheckTreeItem ShortDesc="4.A_1_5" InternalTestName="Fremmednøgletest, uden reference"
                                         Check="ForeignKeyWithNoTarget"
                                         Desc="Der testes for, om der til alle instanser af en fremmednøgle findes en identisk reference i den refererede primærnøgle (Dangling tuples)"
                                         FormattedText="Fremmednøglefejl, manglende reference til primærnøgle {ConstraintName} på fremmednøgle i tabellen {TableName}, {ColumnName} antallet af nøglefejl udgør {Count} %" />
                <viewModel:CheckTreeItem ShortDesc="4.A_1_6" InternalTestName="Fremmednøgletest, del af primærnøgle"
                                         Check="ForeignKeyMissingPrimaryColumns"
                                         Desc="Der testes for, at primærnøglens felter i referencetabellen, er identisk med fremmednøglens felter, herunder den opmærkede rækkefølge af felter"
                                         FormattedText="Fremmednøglefejl, fremmednøglen refererer ikke til hele primærnøglen {ConstraintName} på fremmednøgle i tabellen {TableName}, {ColumnName} " />
                <viewModel:CheckTreeItem ShortDesc="4.A_1_7" InternalTestName="Fremmednøgletest, ikke primærnøgle"
                                         Check="ForeignKeyReferenceNotPrimary"
                                         Desc="Der testes for, om fremmednøglens reference udgøres af en erklæret og opmærket primærnøgle i referencetabellen"
                                         FormattedText="Fremmednøglefejl, fremmednøglen referer ikke til en primærnøgle {ConstraintName} på fremmednøgle i tabellen  {TableName}, {ColumnName}, {ReferenceTable} " />
                <viewModel:CheckTreeItem ShortDesc="4.A_1_8" InternalTestName="Fremmednøgletest, datatyper"
                                         Check="ForeignKeyTypeMismatch"
                                         Desc="Der testes for, om fremmednøglens og referencetabellens felter er identiske i relation til datatype."
                                         FormattedText="Fremmednøgle, manglende overensstemmelse mellem datatyper {ConstraintName} på fremmednøgle i tabellen {TableName}, {ColumnName}, {DataType}, {ReferenceTable}, {ColumnName},{DataType}" />
                <!--        Deprecated        <viewModel:CheckTreeItem ShortDesc="4.A_1_9" Check="ForeignKeyTotalErrors"  FormattedText=""/>-->
                <viewModel:CheckTreeItem ShortDesc="4.A_1_10" InternalTestName="Fremmednøgletest, Blank"
                                         Check="ForeignKeyBlanks"
                                         Desc="Der testes for, om fremmednøglefelter starter eller afslutter, med en eller anden form for blanktegn"
                                         FormattedText="Fremmednøglefejl. Nøgle begynder eller afslutter med et blanktegn {ConstraintName}, {TableName}, {ColumnName} antallet af nøglefejl udgør {Count} % " />
                <viewModel:CheckTreeItem ShortDesc="4.A_1_11" IsError="False"
                                         InternalTestName="Fremmednøgletest, tomme fremmednøgler"
                                         Check="ForeignKeyNullCount"
                                         Desc="Der laves en optælling af antallet af null-værdier i hver fremmednøgle."
                                         FormattedText="Tomme fremmednøgler i {ConstraintName} i tabellen {TableName} . {Count} tomme fremmednøgler ud af i alt {Count} rækker, {Count} %. " />
            </viewModel:CheckTreeItem>
            <viewModel:CheckTreeItem ShortDesc="4.B.1 Placering af mapper og filer">
                <viewModel:CheckTreeItem.Desc>
                    <system:String xml:space="preserve">I roden af filsystemet på afleveringsmediet, jf. bilag 7, skal der være placeret en mappe navngivet med mediets navn. Mediets navn består af det unikke arkiveringsversionsID med tilføjelse af et suffiks ».n«, der angiver rækkefølgen af medierne, hvor n er et fortløbende medieløbenummer begyndende med 1.</system:String>
                </viewModel:CheckTreeItem.Desc>
                <viewModel:CheckTreeItem ShortDesc="4.B.1_1"
                                         InternalTestName="Mappestrukturtest, manglende første medie"
                                         Check="FolderStructureFirstMediaMissing"
                                         Desc="Der testes for om der findes et medie, som er korrekt navngivet, inklusiv medieløbenummer"
                                         FormattedText="Første medie findes ikke" />
                <viewModel:CheckTreeItem ShortDesc="4.B.1_2"
                                         InternalTestName="Mappestrukturtest, samme medieløbenummer"
                                         Check="FolderStructureMediaNumberGaps"
                                         Desc="Der testes for den fortløbende nummerering af medier."
                                         FormattedText="Der findes medier med samme medieløbenummer {Path}" />
                <viewModel:CheckTreeItem ShortDesc="4.B.1_3" InternalTestName="Mappestrukturtest, rækkefølge"
                                         Check="FolderStructureDuplicateMediaNumber"
                                         Desc="Der testes for, om rækkefølgen af medier er ubrudt"
                                         FormattedText="Der er en fejl i rækkefølgen af Medier" />
            </viewModel:CheckTreeItem>
            <viewModel:CheckTreeItem ShortDesc="4.B.2 Arkiveringsversionens indhold fordeles i mapper">
                <viewModel:CheckTreeItem.Desc>
                    <system:String xml:space="preserve">4.B.2	Arkiveringsversionens indhold fordeles i mapper, som angivet i figur 4.1.
4.B.3	Mapperne skal navngives som angivet i figur 4.1.
4.B.4.a	Et arkiveringsversionsID består af præfikset AVID, en kode på 2-4 bogstaver, som angiver det modtagende arkiv, samt et arkiveringsversionsløbenummer. Elementerne adskilles med punktum.
4.B.5.a	En arkiveringsversion, som ikke kan være på ét medie, jf. bilag 7, kan fordeles over flere medier.
4.B.5.b	Mapperne ContextDocumentation, Indices og Schemas skal altid være placeret på afleveringens første medie.
4.B.5.c	Mappen i roden på efterfølgende medier skal kun indeholde de mapper, hvis indhold kræver fordeling over flere medier. Disse mapper navngives altid som angivet i figur 4.1 uden brug af suffiks.
4.B.6	Der må kun være én af hver af de i figur 4.1 angivne mapper på hvert medie.</system:String>
                </viewModel:CheckTreeItem.Desc>
                <viewModel:CheckTreeItem ShortDesc="4.B.2_1" InternalTestName="Mappestruktur, Indices"
                                         Check="FolderStructureMissingIndicesFirstMedia"
                                         Desc="Der testes for om mappen Indices findes på første medie"
                                         FormattedText="Der findes ikke en Indices mappe på første medie" />
                <viewModel:CheckTreeItem ShortDesc="4.B.2_2" InternalTestName="Mappestruktur, Schemas"
                                         Check="FolderStructureSchemasMissing"
                                         Desc="Der testes for om mappen Schemas findes på første medie"
                                         FormattedText="Der findes ikke en Schemas mappe på første medie" />
                <viewModel:CheckTreeItem ShortDesc="4.B.2_3" InternalTestName="Mappestruktur, ContextDocumentation"
                                         Check="FolderStructureContextDocsMissing"
                                         Desc="Der testes for om mappen ContextDocumentation findes på første medie"
                                         FormattedText="Der findes ikke en ContextDocumentation mappe på første medie" />
                <viewModel:CheckTreeItem ShortDesc="4.B.2_4" InternalTestName="Mappestruktur, Tables mangler"
                                         Check="FolderStructureTablesMissing"
                                         Desc="Der testes for om der i hele arkiveringsversionen findes en mappe, navngivet som Tables"
                                         FormattedText="Der findes ikke en Tables mappe i arkiveringsversionen" />
                <viewModel:CheckTreeItem ShortDesc="4.B.2_5" InternalTestName="Mappestruktur, flere indices"
                                         Check="FolderStructureIndicesWrongMedia"
                                         Desc="Der testes for, om der findes flere Indicesmapper, end den der skal ligge på første medie"
                                         FormattedText="Der findes mere end 1 Indices mappe i arkiveringsversionen {MediaNumber}" />
                <viewModel:CheckTreeItem ShortDesc="4.B.2_6" InternalTestName="Mappestruktur, flere Schemas"
                                         Check="FolderStructureSchemasWrongMedia"
                                         Desc="Der testes for, om der findes flere Schemamapper, end den der skal ligge på første medie"
                                         FormattedText="Der findes mere end 1 Schemas mappe i arkiveringsversionen {MediaNumber}" />
                <viewModel:CheckTreeItem ShortDesc="4.B.2_7"
                                         InternalTestName="Mappestruktur, flere ContextDocumentation"
                                         Check="FolderStructureContextDocsWrongMedia"
                                         Desc="Der testes for, om der findes flere ContextDocumentationmapper, end den der skal ligge på første medie"
                                         FormattedText="Der findes mere end 1 ContextDocumentations mappe i arkiveringsversionen {MediaNumber}" />
                <viewModel:CheckTreeItem ShortDesc="4.B.2_8" IsError="False"
                                         InternalTestName="Mappestruktur, Documents"
                                         Check="FolderStructureMultipleDocuments"
                                         Desc="Der undersøges om arkiveringsversionen indeholder en eller flere mapper, navngivet som Documents"
                                         FormattedText="Der findes en Documents mappe på {MediaNumber}" />
                <viewModel:CheckTreeItem ShortDesc="4.B.2_9" IsError="False" InternalTestName="Mappestruktur, Tables"
                                         Check="FolderStructureMultipleTables"
                                         Desc="Der undersøges om arkiveringsversionen indeholder en eller flere mapper navngivet som Tables"
                                         FormattedText="Der findes en Tables mappe på {MediaNumber}" />
            </viewModel:CheckTreeItem>
            <viewModel:CheckTreeItem ShortDesc="4.C. Mappen Indices">
                <viewModel:CheckTreeItem.Desc>
                    <system:String xml:space="preserve">Der testes for om mappen Indices findes på første medie</system:String>
                </viewModel:CheckTreeItem.Desc>
                <viewModel:CheckTreeItem ShortDesc="4.C_1" InternalTestName="Indicestest, fileIndex"
                                         Check="IndicesFileIndex"
                                         Desc="Der testes for om mappen Indices indeholder en fileIndex.xml"
                                         FormattedText="Der mangler en fileIndex.xml i arkiveringsversionen" />
                <viewModel:CheckTreeItem ShortDesc="4.C_2" InternalTestName="Indicestest,archiveIndex"
                                         Check="IndicesArchiveIndex"
                                         Desc="Der testes for om mappen Indices indeholder en archiveIndex.xml"
                                         FormattedText="Der mangler en archiveIndex.xml i arkiveringsversionen" />
                <viewModel:CheckTreeItem ShortDesc="4.C_3" InternalTestName="Indicestest,ContextDocumentatationIndex"
                                         Check="IndicesContextDocIndex"
                                         Desc="Der testes for om mappen Indices indeholder en ContextDocumentatationIndex.xml"
                                         FormattedText="Der mangler en ContextDocumentatationIndex.xml i arkiveringsversionen" />
                <viewModel:CheckTreeItem ShortDesc="4.C_4" InternalTestName="Indicestest, tableIndex"
                                         Check="IndicesTableIndex"
                                         Desc="Der testes for om mappen Indices indeholder en tableIndex.xml"
                                         FormattedText="Der mangler en tableIndex.xml i arkiveringsversionen" />
                <viewModel:CheckTreeItem ShortDesc="4.C_5" InternalTestName="Indicestest, docIndex"
                                         Check="IndicesDocIndex"
                                         Desc="Der testes for om mappen Indices indeholder en docIndex.xml"
                                         FormattedText="Der mangler en docIndex.xml i arkiveringsversionen" />
            </viewModel:CheckTreeItem>
            <viewModel:CheckTreeItem ShortDesc="4.C.1  Indexfileroverholder skema">
                <viewModel:CheckTreeItem.Desc>
                    <system:String xml:space="preserve">4.C.1.c	Alle indeksfiler skal overholde deres tilhørende skema, jf. bilag 8.</system:String>
                </viewModel:CheckTreeItem.Desc>
                <viewModel:CheckTreeItem ShortDesc="4.C.1_1" InternalTestName="Skematest, fileIndex well-formed"
                                         Check="FileIndexNotWellFormed"
                                         Desc="Der testes for om fileIndex.xml er well-formed"
                                         FormattedText="Der findes en well-formed fejl i filIndex.xml{ErrorType}" />
                <viewModel:CheckTreeItem ShortDesc="4.C.1_2" InternalTestName="Skematest, fileIndex valid"
                                         Check="FileIndexInvalid" Desc="Der testes for om fileIndex.xml er valid"
                                         FormattedText="Der findes en validitetsfejl i filIndex.xml{ErrorType}" />
                <viewModel:CheckTreeItem ShortDesc="4.C.1_3" InternalTestName="Skematest, archiveIndex well-formed"
                                         Check="ArchiveIndexNotWellFormed"
                                         Desc="Der testes for om archiveIndex.xml er well-formed"
                                         FormattedText="Der findes en well-formed fejl i archiveIndex.xml{ErrorType}" />
                <viewModel:CheckTreeItem ShortDesc="4.C.1_4" InternalTestName="Skematest, archiveIndex valid"
                                         Check="ArchiveIndexInvalid" Desc="Der testes for om archiveIndex.xml er valid"
                                         FormattedText="Der findes en validitetsfejl i archiveIndex.xml{ErrorType}" />
                <viewModel:CheckTreeItem ShortDesc="4.C.1_5"
                                         InternalTestName="Skematest, ContextDocumentatation well-formed"
                                         Check="ContextDocumentationNotWellFormed"
                                         Desc="Der testes for om ContextDocumentation.xml er well-formed"
                                         FormattedText="Der findes en well-formed fejl i ContextDocumentation.xml{ErrorType}" />
                <viewModel:CheckTreeItem ShortDesc="4.C.1_6" InternalTestName="Skematest, ContextDocumentation valid"
                                         Check="ContextDocumentationInvalid"
                                         Desc="Der testes for om ContextDocumentation.xml er valid"
                                         FormattedText="Der findes en validitetsfejl i ContextDocumentation.xml{ErrorType}" />
                <viewModel:CheckTreeItem ShortDesc="4.C.1_7" InternalTestName="Skematest, tableIndex well-formed"
                                         Check="TableIndexNotWellFormed"
                                         Desc="Der testes for om tableIndex.xml er well-formed"
                                         FormattedText="Der findes en well-formed fejl i tableIndex.xml{ErrorType}" />
                <viewModel:CheckTreeItem ShortDesc="4.C.1_8" InternalTestName="Skematest, tableIndex valid"
                                         Check="TableIndexInvalid" Desc="Der testes for om tableIndex.xml er valid"
                                         FormattedText="Der findes en validitetsfejl i tableIndex.xml{ErrorType}" />
                <viewModel:CheckTreeItem ShortDesc="4.C.1_9" InternalTestName="Skematest, docIndex well-formed"
                                         Check="DocIndexNotWellFormed"
                                         Desc="Der testes for om docIndex.xml er well-formed"
                                         FormattedText="Der findes en well-formed fejl i docIndex.xml{ErrorType}" />
                <viewModel:CheckTreeItem ShortDesc="4.C.1_10" InternalTestName="Skematest, docIndex valid"
                                         Check="DocIndexInvalid" Desc="Der testes for om docIndex.xml er valid"
                                         FormattedText="Der findes en validitetsfejl i docIndex.xml{ErrorType}" />
                <viewModel:CheckTreeItem ShortDesc="4.C.1_11" IsError="False"
                                         InternalTestName="Skematest, UTF8 deklaration"
                                         Check="AdaAvXmlIndexIllegalEncoding"
                                         Desc="Der testes for, at indeksfilerne indeholder en korrekt opmærket XML deklaration med angivelse af UTF8 tegnsæt"
                                         FormattedText="Indeksfilen {SchemaName} , {Path} er ikke opmærket korrekt, angivelsen af tegnsæt er forkert" />
                <viewModel:CheckTreeItem ShortDesc="4.C.1_12" IsError="False" InternalTestName="Nope"
                                         Check="AdaAvXmlIndexMissingProlog" Desc="" FormattedText="" />
            </viewModel:CheckTreeItem>
            <viewModel:CheckTreeItem ShortDesc="4.C.2 fileIndex.xml">
                <viewModel:CheckTreeItem.Desc>
                    <system:String xml:space="preserve">4.C.2.a	fileIndex.xml skal indeholde en komplet liste over samtlige filer, der findes i arkiveringsversionen. fileIndex.xml er dog undtaget fra denne regel.
4.C.2.b	For hver enkelt fil i arkiveringsversionen angives de oplysninger, som fremgår af figur 4.2.</system:String>
                </viewModel:CheckTreeItem.Desc>
                <viewModel:CheckTreeItem ShortDesc="4.C.2_1" InternalTestName="fileIndex, filindhold manglende filer"
                                         Check="FileIndexFilesMissing"
                                         Desc="Der testes for, om alle de filer der er opmærket i fileIndex.xml, findes fysisk i arkiveringsversion."
                                         FormattedText="Filen {FileName} som er opmærket i fileIndex.xml, kan ikke findes i arkiveringsversionen" />
                <viewModel:CheckTreeItem ShortDesc="4.C.2_2" InternalTestName="fileIndex, filIndholdoverskydende filer"
                                         Check="FileIndexFilesNotInIndex"
                                         Desc="Der testes for, om alle de filer der fysisk befinder sig i arkiveringsversionen, er opmærket i fileIndex.xml."
                                         FormattedText="Filen {FileName}, som findes i arkiveringsversion, {Path} er ikke opmærket i fileIndex.xml" />
                <viewModel:CheckTreeItem ShortDesc="4.C.2_3" InternalTestName="fileIndex, kontrolsum"
                                         Check="FileIndexBadCheckSum"
                                         Desc="Der testes for, om den enkelte fil i arkiveringsversionen, som er korrekt opmærket i fileIndex.xml, har en identisk kontrolsum iflg. den opmærkede værdi."
                                         FormattedText="Filen {FileName}, {Path} har en afvigende checksum fra den værdi, der er opmærket i fileIndex.xml" />
                <viewModel:CheckTreeItem ShortDesc="4.C.2_4" InternalTestName="fileIndex, AVID" Check="FileIndexAvid"
                                         Desc="Der testes for, at arkiveringsversionsID angivet i &lt;foN&gt; er identisk med arkiveringsversionsID."
                                         FormattedText="Filen {FileName} som er opmærket i fileIndex.xml, stemmer ikke overens med arkiveringsversionens ID." />
                <viewModel:CheckTreeItem ShortDesc="4.C.2_5" InternalTestName="fileIndex, dubletter"
                                         Check="FileIndexDuplet"
                                         Desc="Der testes for om fileIndex.xml indeholder dubletopmærkninger for samme fil."
                                         FormattedText="Filen {FileName} som er opmærket i fileIndex.xml er opmærket mere end en gang." />
            </viewModel:CheckTreeItem>
            <viewModel:CheckTreeItem ShortDesc="4.C.4 contextDocumentationIndex.xml">
                <viewModel:CheckTreeItem.Desc>
                    <system:String xml:space="preserve">4.C.4.a	contextDocumentationIndex.xml skal indeholde et indeks over de dokumenter, som findes i arkiveringsversionens kontekstdokumentation.
4.C.4.b	For hvert dokument i kontekstdokumentationen angives de oplysninger, som fremgår af figur 4.3.</system:String>
                </viewModel:CheckTreeItem.Desc>
                <viewModel:CheckTreeItem ShortDesc="4.C.4_1"
                                         InternalTestName="contextDocumentationIndex.xml, kategorier"
                                         Check="ContextDocIndexDocMissingCategory"
                                         Desc="Der testes for, at mindst 1 af dokumentkategorierne i &lt;documentCategory&gt; contextDocumentationIndex.xml er markeret som ''true'' for hvert dokument"
                                         FormattedText="Der er ikke opmærket en dokumentkategori for følgende dokument: {DocumentID}" />
            </viewModel:CheckTreeItem>
            <viewModel:CheckTreeItem ShortDesc="4.C.5  tableIndex.xml">
                <viewModel:CheckTreeItem.Desc>
                    <system:String xml:space="preserve">4.C.5.a	tableIndex.xml skal indeholde en angivelse af en relationel databasestruktur på 1. normalform eller højere. Samtlige tabeller i arkiveringsversionen skal angives.
4.C.5.b	»tableIndex.xml« skal overholde det generelle XML-skema »tableIndex.xsd«, jf. 4.F.
4.C.5.c	Hvis et felt må have værdien NULL, skal der i »tableIndex.xml« være angivet værdien »true« for kolonnens tilhørende element »nullable«.</system:String>
                </viewModel:CheckTreeItem.Desc>
                <viewModel:CheckTreeItem ShortDesc="4.C.5_1" IsError="False"
                                         InternalTestName="Relationstest, tabeller uden relation"
                                         Check="TableIndexNoRelations"
                                         Desc="Der undersøges, om der findes tabeller uden relation til andre tabeller"
                                         FormattedText="Tabellerne {TableNames} har ingen relation til andre tabeller i arkiveringsversionen" />
                <viewModel:CheckTreeItem ShortDesc="4.C.5_2" InternalTestName="Tabeltest, nullable"
                                         Check="TablePrimaryKeyIsNullable"
                                         Desc="Der undersøges, om de felter der indgår i en primærnøgle, er opmærket med nullable = true"
                                         FormattedText="Der findes en tabel {TableName}, {FolderNumber}, hvor dele af nøglen {ColumnName}, {ColumnNumber} er opmærket som NULLABLE" />
                <viewModel:CheckTreeItem ShortDesc="4.C.5_3" InternalTestName="Tabeltest, tabelnavne unikke"
                                         Check="TableNameDuplicate"
                                         Desc="Der undersøges, om alle tabeller i en arkiveringsversion har et unikt navn i tableIndex.xml"
                                         FormattedText="Der findes flere tabeller i arkiveringsversionen, som er opmærket med det samme navn. Første gang det optræder, er med: {TableName}, {FolderNumber}," />
            </viewModel:CheckTreeItem>
            <viewModel:CheckTreeItem ShortDesc="4.C.6 docIndex.xml">
                <viewModel:CheckTreeItem.Desc>
                    <system:String xml:space="preserve">4.C.6.a	docIndex.xml skal danne forbindelsen mellem hvert dokument og dets placering. »docIndex.xml« skal desuden indeholde oplysninger om dokumenternes oprindelige filnavne, filtype i arkiveringsversionen samt eventuelle overordnede dokumenter. »docIndex.xml« skal ikke indeholde oplysninger om dokumenterne i kontekstdokumentationen.
4.C.6.b	For hvert enkelt dokument i docIndex.xml angives de oplysninger, som fremgår af figur 4.4.</system:String>
                </viewModel:CheckTreeItem.Desc>
                <viewModel:CheckTreeItem ShortDesc="4.C.6_1" IsError="False"
                                         InternalTestName="Dokumentindekstest, parentID" Check="DocIndexNestedDocCount"
                                         Desc="Der testes for, om der findes opmærkninger i docIndex.xml i elementet &lt;pID&gt;"
                                         FormattedText="Arkiveringsversionen har {NestedDocuments} % indlejrede dokumenter &lt;pID&gt; ud af det samlede antal dokumenter {Count}" />
                <viewModel:CheckTreeItem ShortDesc="4.C.6_2" InternalTestName="Dokumentindekstest, GML"
                                         Check="DocIndexInvalidGmlXsd"
                                         Desc="Hvis arkiveringsversionen har opmærket ekstension ''.gml'' i oFn samt gml i aFt testes der for, om opmærkningen i gmlXsd er udført korrekt."
                                         FormattedText="Referencen til GML-filens skema er ikke opmærket korrekt. {GmlPath}" />
